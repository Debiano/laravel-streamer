{
  "name": "prwnr/laravel-streamer",
  "description": "Events streaming package for Laravel that uses Redis 5 streams",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rafal Purwin",
      "email": "purwinr@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/console": "5.6.*|5.7.*",
    "illuminate/support": "5.6.*|5.7.*",
    "illuminate/redis": "5.6.*|5.7.*",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Prwnr\\Streamer\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "aliases": {
        "Streamer": "Prwnr\\Streamer\\Facades\\Streamer"
      },
      "providers": [
        "Prwnr\\Streamer\\StreamerProvider"
      ]
    }
  }
}
